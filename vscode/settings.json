{
    "window.zoomLevel": 1,
    "git.autofetch": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "python.testing.promptToConfigure": false,
    "python.testing.pytestEnabled": false,
    "python.testing.unittestEnabled": false,
    "python.testing.nosetestsEnabled": false,
    "python.pythonPath": "/usr/local/bin/python3",
    "python.linting.pylintEnabled": true,
    "python.testing.pytestArgs": [
        "-vv",
        "--tb=native"
    ],
    "go.autocompleteUnimportedPackages": true,
    "go.testTimeout": "600s",
    "go.useLanguageServer": true,
    "go.languageServerFlags": [],
    "workbench.colorTheme": "Complete Dark",
    "editor.fontFamily": "FiraCode-Retina",
    "editor.fontLigatures": true,
    "editor.fontWeight": "500", // Light
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "name": "Heading",
                "scope": "markup.heading",
                "settings": {
                    "fontStyle": "",
                    "foreground": "#00FF00"
                }
            },
            {
                "name": "Bold",
                "scope": "markup.bold.yandex.wiki",
                "settings": {
                    "fontStyle": "bold",
                    "foreground": "#f92672"
                }
            },
            {
                "name": "Italic",
                "scope": "markup.italic.yandex.wiki",
                "settings": {
                    "fontStyle": "italic",
                    "foreground": "#e42e70"
                }
            },
            {
                "name": "Underline",
                "scope": "markup.underline.yandex.wiki",
                "settings": {
                    "fontStyle": "underline",
                    "foreground": "#e42e70"
                }
            },
            {
                "name": "Question",
                "scope": "markup.yandex.wiki.question",
                "settings": {
                    "fontStyle": "italic",
                    "foreground": "#ff0000"
                }
            },
            {
                "name": "Link URL",
                "scope": "markup.underline.link.yandex.wiki",
                "settings": {
                    "fontStyle": "",
                    "foreground": "#999"
                }
            },
            {
                "name": "Link Text",
                "scope": "markup.underline.text.yandex.wiki",
                "settings": {
                    "fontStyle": "",
                    "foreground": "#e6db74ff"
                }
            },
            {
                "name": "Quote",
                "scope": "markup.quote.yandex.wiki",
                "settings": {
                    "foreground": "#800000"
                }
            },
            {
                "name": "Remark green",
                "scope": "markup.yandex.wiki.remark.green",
                "settings": {
                    "foreground": "#00ff00"
                }
            },
            {
                "name": "Remark red",
                "scope": "markup.yandex.wiki.remark.red",
                "settings": {
                    "foreground": "#ff0000"
                }
            },
            {
                "name": "Remark blue",
                "scope": "markup.yandex.wiki.remark.blue",
                "settings": {
                    "foreground": "#04b"
                }
            },
            {
                "name": "Remark gray",
                "scope": "markup.yandex.wiki.remark.gray",
                "settings": {
                    "foreground": "#808080"
                }
            },
            {
                "name": "Remark yellow",
                "scope": "markup.yandex.wiki.remark.yellow",
                "settings": {
                    "foreground": "#FFFF00"
                }
            },
            {
                "name": "Code block",
                "scope": "markup.inline.raw.string, punctuation.definition.yandex.wiki",
                "settings": {
                    "foreground": "#fd971fff"
                }
            },
            {
                "name": "Custom",
                "scope": "markup.italic.yandex.qqq",
                "settings": {
                    "fontStyle": "italic",
                    "foreground": "#e42e70"
                }
            }
        ]
    },
    "terminal.integrated.rendererType": "dom",
    "workbench.editor.showTabs": false,
    "editor.minimap.enabled": false,
    "[markdown]": {
        "editor.wordWrap": "on",
        "editor.quickSuggestions": true
    },
    "python.formatting.provider": "black",
    "python.formatting.blackPath": "/usr/local/bin/black",
    "gitlens.views.repositories.files.layout": "tree",
    "editor.detectIndentation": false,
    "vscode-yandex-tracker.query": "\"Queue\": \"YANDEXPAY\"",
    "go.buildOnSave": "off",
    "go.formatTool": "gofmt",
    "editor.formatOnSave": false,
    "vscode-yandex-tracker.organizationId": "2719817",
    "vscode-yandex-tracker.host": "https://st-api.yandex-team.ru",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "files.autoSave": "afterDelay",
    "files.trimTrailingWhitespace": true,
    "git.ignoreLegacyWarning": true,
    "extensions.autoUpdate": true,
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "dart.flutterSdkPath": "/Users/perseus/Downloads/flutter",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[sql]": {
        "editor.defaultFormatter": "bradymholt.pgformatter"
    },
    "python.autoComplete.extraPaths": [],
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "scm.defaultViewMode": "tree",
    "git.decorations.enabled": false,
    "editor.maxTokenizationLineLength": 200000,
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "code-runner.enableAppInsights": false,
    "gitlens.advanced.telemetry.enabled": false,
    "githubPullRequests.telemetry.enabled": false,
    "python.languageServer": "Pylance",
    "vscode-yandex-arc.enableLog": true,
    "python.analysis.extraPaths": [],
    "python.showStartPage": false,
    "vscode-yandex-arc.gutterDiffTarget": "HEAD",
    "go.testFlags": [
        "-v"
    ],
    "vscode-yandex-arc.enableAutoupdate": true,
    "explorer.confirmDragAndDrop": false,
    "mssql.connections": [
        {
            "server": "{{put-server-name-here}}",
            "database": "{{put-database-name-here}}",
            "user": "{{put-username-here}}",
            "password": ""
        }
    ],
    "workbench.editor.decorations.colors": true,
    "workbench.editor.decorations.badges": true,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/*.code-search": true,
        "**/*.pyc": true,
    },
    "search.mode": "newEditor",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "typescript.tsserver.log": "verbose",
    "typescript.tsserver.trace": "verbose",
    "[vue]": {
        "editor.defaultFormatter": "octref.vetur"
    },
    "go.toolsManagement.autoUpdate": true,
    "python.linting.pylintPath": "/usr/local/bin/pylint",
    "vscode-yandex-arc.updateChannel": "stable",
    "diffEditor.ignoreTrimWhitespace": false,
    "redhat.telemetry.enabled": false,
    "editor.rulers": [
        120
    ],
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "explorer.confirmDelete": false,
    "security.workspace.trust.untrustedFiles": "open",
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "numberedBookmarks.navigateThroughAllFiles": "replace",
    "python.linting.flake8Enabled": true,
    "python.testing.autoTestDiscoverOnSaveEnabled": false,
    "go.testExplorer.enable": false,
    "editor.unicodeHighlight.ambiguousCharacters": false
}

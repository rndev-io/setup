{
    "window.zoomLevel": 1,
    "git.autofetch": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "python.testing.nosetestsEnabled": false,
    "python.jediEnabled": false,
    "python.pythonPath": "/usr/local/bin/python3.7",
    "python.linting.pylintEnabled": true,
    "python.testing.pytestArgs": [
        "-vv",
        "--tb=native"
    ],
    "go.autocompleteUnimportedPackages": true,
    "go.testTimeout": "600s",
    "go.useLanguageServer": true,
    "go.languageServerFlags": [],
    "go.testFlags": [
        "-v"
    ],
    "workbench.statusBar.feedback.visible": false,
    "workbench.colorTheme": "Monokai",
    "editor.fontFamily": "FiraCode-Retina",
    "editor.fontLigatures": true,
    "editor.fontWeight": "500", // Light
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "name": "Heading",
                "scope": "markup.heading",
                "settings": {
                    "fontStyle": "",
                    "foreground": "#00FF00"
                }
            },
            {
                "name": "Bold",
                "scope": "markup.bold.yandex.wiki",
                "settings": {
                    "fontStyle": "bold",
                    "foreground": "#f92672"
                }
            },
            {
                "name": "Italic",
                "scope": "markup.italic.yandex.wiki",
                "settings": {
                    "fontStyle": "italic",
                    "foreground": "#e42e70"
                }
            },
            {
                "name": "Underline",
                "scope": "markup.underline.yandex.wiki",
                "settings": {
                    "fontStyle": "underline",
                    "foreground": "#e42e70"
                }
            },
            {
                "name": "Question",
                "scope": "markup.yandex.wiki.question",
                "settings": {
                    "fontStyle": "italic",
                    "foreground": "#ff0000"
                }
            },
            {
                "name": "Link URL",
                "scope": "markup.underline.link.yandex.wiki",
                "settings": {
                    "fontStyle": "",
                    "foreground": "#999"
                }
            },
            {
                "name": "Link Text",
                "scope": "markup.underline.text.yandex.wiki",
                "settings": {
                    "fontStyle": "",
                    "foreground": "#e6db74ff"
                }
            },
            {
                "name": "Quote",
                "scope": "markup.quote.yandex.wiki",
                "settings": {
                    "foreground": "#800000"
                }
            },
            {
                "name": "Remark green",
                "scope": "markup.yandex.wiki.remark.green",
                "settings": {
                    "foreground": "#00ff00"
                }
            },
            {
                "name": "Remark red",
                "scope": "markup.yandex.wiki.remark.red",
                "settings": {
                    "foreground": "#ff0000"
                }
            },
            {
                "name": "Remark blue",
                "scope": "markup.yandex.wiki.remark.blue",
                "settings": {
                    "foreground": "#04b"
                }
            },
            {
                "name": "Remark gray",
                "scope": "markup.yandex.wiki.remark.gray",
                "settings": {
                    "foreground": "#808080"
                }
            },
            {
                "name": "Remark yellow",
                "scope": "markup.yandex.wiki.remark.yellow",
                "settings": {
                    "foreground": "#FFFF00"
                }
            },
            {
                "name": "Code block",
                "scope": "markup.inline.raw.string, punctuation.definition.yandex.wiki",
                "settings": {
                    "foreground": "#fd971fff"
                }
            },
            {
                "name": "Custom",
                "scope": "markup.italic.yandex.qqq",
                "settings": {
                    "fontStyle": "italic",
                    "foreground": "#e42e70"
                }
            }
        ]
    },
    "terminal.integrated.rendererType": "dom",
    "workbench.editor.showTabs": false,
    "editor.minimap.enabled": false,
    "breadcrumbs.enabled": false,
    "[markdown]": {
        "editor.wordWrap": "on",
        "editor.quickSuggestions": true
    },
    "projectManager.vscode.baseFolders": [
        "/Users/perseus/Projects/private"
    ],
    "projectManager.any.maxDepthRecursion": 1,
    "projectManager.git.maxDepthRecursion": 1,
    "gitlens.views.repositories.files.layout": "tree",
    "editor.detectIndentation": false,
    "cSpell.language": ",ru",
    "vscode-yandex-tracker.query": "\"Queue\": \"BCL\"",
    "go.buildOnSave": "off",
    "go.formatTool": "gofmt",
    "editor.formatOnSave": true,
    "vscode-yandex-tracker.organizationId": "2719817",
    "vscode-yandex-tracker.host": "https://st-api.test.yandex-team.ru",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "files.autoSave": "afterDelay",
    "git.ignoreLegacyWarning": true,
    "extensions.autoUpdate": true,
    "svn.enableProposedApi": "product",
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "dart.flutterSdkPath": "/Users/perseus/Downloads/flutter",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[sql]": {
        "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
    },
    "python.autoComplete.extraPaths": [],
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "scm.defaultViewMode": "tree",
    "git.decorations.enabled": false,
    "editor.maxTokenizationLineLength": 200000,
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    }
}